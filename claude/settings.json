{
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/scripts/notify.sh",
            "timeout": 10
          },
          {
            "type": "command",
            "command": "~/.local/bin/claude-notify",
            "timeout": 10
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/scripts/notify-stop.sh",
            "timeout": 10
          },
          {
            "type": "command",
            "command": "~/.local/bin/claude-notify",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "node /Users/socoolbear/.claude/hud/omc-hud.mjs"
  },
  "enabledPlugins": {
    "typescript-lsp@claude-plugins-official": true,
    "document-skills@anthropic-agent-skills": true,
    "opgg-essentials@opgg-plugins": true,
    "oh-my-claudecode@omc": true,
    "mgrep@Mixedbread-Grep": true
  },
  "alwaysThinkingEnabled": true
}
